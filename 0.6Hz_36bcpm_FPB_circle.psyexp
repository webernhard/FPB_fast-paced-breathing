        <Param val="# ===== BEGIN ROUTINE Tab =====&amp;#10;# Dieser Code kommt in den &quot;Begin Routine&quot; Tab&amp;#10;&amp;#10;#from psychopy import visual, core&amp;#10;&amp;#10;print(&quot;=== ATEMBALL ROUTINE STARTET ===&quot;)&amp;#10;&quot;&quot;&quot;&amp;#10;# Erstelle die visuellen Komponenten (nutzt das bestehende win-Objekt)&amp;#10;breathing_circle = visual.Circle(&amp;#10;    win,&amp;#10;    #win=win,&amp;#10;    radius=MAX_RADIUS * MIN_SIZE_PERCENT,&amp;#10;    edges=64,&amp;#10;    lineColor=[1, 1, 1],&amp;#10;    lineWidth=3,&amp;#10;    pos=[0, 0]  # Position anpassen falls nötig&amp;#10;)&amp;#10;&amp;#10;breathing_text = visual.TextStim(&amp;#10;    win,&amp;#10;    #win=win,&amp;#10;    text='',&amp;#10;    pos=[0, -350],  # Position unter dem Ball&amp;#10;    #height=40, #px&amp;#10;    height=.35, # unit: norm&amp;#10;    color=[1, 1, 1]&amp;#10;)&amp;#10;&quot;&quot;&quot;&amp;#10;#print(&quot;Komponenten erstellt, Timer gestartet&quot;)&amp;#10;&amp;#10;b_start = breathing_clock.getTime()&amp;#10;print(f'{b_start=}')&amp;#10;print(&quot;Timer gestartet&quot;)&amp;#10;&amp;#10;&amp;#10;&quot;&quot;&quot;&amp;#10;## ===== AUSFÜHRUNG =====&amp;#10;#if __name__ == '__main__':&amp;#10;#    # Starte für 60 Sekunden (oder endlos mit duration=0)&amp;#10;#    run_breathing_ball(duration=30)&amp;#10;print(&quot;go 4 it&quot;)&amp;#10;run_breathing_ball(duration=30)&amp;#10;&quot;&quot;&quot;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&quot;&quot;&quot;&amp;#10;&amp;#10;win.mouseVisible = False&amp;#10;&amp;#10;c_resp_txt.alignText = 'center'&amp;#10;&amp;#10;##  &gt; 'Einatmen'&amp;#10;resp_txt = 'Einatmen'&amp;#10;b_txt_size = 0.035&amp;#10;&amp;#10;circle_color = inhale_color         #(re)set color of breathing bar&amp;#10;&amp;#10;respTime = 0&amp;#10;respClock.reset()&amp;#10;&amp;#10;exhale = False&amp;#10;&amp;#10;HVonce = False&amp;#10;#print(&quot;current HV-loop:&quot;, (HV_loop.thisN, HV_loops, HV_loop.thisTrialN))&amp;#10;&amp;#10;tonce = False&amp;#10;&quot;&quot;&quot;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Py" valType="str" updates="None" name="Code Type"/>
        <Param val="# Berechne verstrichene Zeit&amp;#10;elapsed = breathing_clock.getTime()&amp;#10;&amp;#10;# Hole aktuellen Zustand&amp;#10;radius, color, phase = get_breathing_state(elapsed)&amp;#10;&amp;#10;# Aktualisiere Kreis&amp;#10;breathing_circle.radius = radius&amp;#10;breathing_circle.fillColor = color&amp;#10;&amp;#10;# Aktualisiere Text (optional)&amp;#10;breathing_text.text = phase&amp;#10;&amp;#10;# Zeichne die Komponenten&amp;#10;breathing_circle.draw()&amp;#10;breathing_text.draw()&amp;#10;&amp;#10;core.wait(.5)&amp;#10;&amp;#10;# Debug-Output (nur ersten Frame und dann alle 60 Frames)&amp;#10;if frameN == 0 or frameN % 60 == 0:&amp;#10;    print(f&quot;Frame {frameN}: Zeit={elapsed:.2f}s, Phase={phase}, Radius={radius:.3f}&quot;)&amp;#10;&amp;#10;&amp;#10;# Beende Routine nach festgelegter Zeit&amp;#10;#if elapsed &gt;= b_start + breathing_duration:&amp;#10;#    continueRoutine = False&amp;#10;&amp;#10;&amp;#10;&quot;&quot;&quot;&amp;#10;try:&amp;#10;    ##  get current (respiration-animation) time  ##&amp;#10;    respTime = respClock.getTime()&amp;#10;&amp;#10;    # # # respiration text: basic-size = 0.035  ##&amp;#10;    # # #    '/50' &gt; flatten the (cos-)curve; +1/50 &gt; offset to start x=0|y=0&amp;#10;    # # # b_txt_size = 0.035 -cos(pi*respTime/(dur_inhaleIHOH+durHold))/50 +1/50 #&amp;#10;&amp;#10;    ##  respiration circle: res(idual)-size = 200px  ##&amp;#10;    ##    '+|- '150' &gt; variability of circle size&amp;#10;    circ_dia = res_size -cosXYshift*cos(pi*respTime/(dur_inhaleIHOH+durHold)) + cosXYshift&amp;#10;&amp;#10;    ##  &gt; hold&amp;#10;    #if respTime &gt;= dur_inhaleIHOH: resp_txt = ' '  #no 'hold' for HV&amp;#10;&amp;#10;    ##  &gt; 'Ausatmen'&amp;#10;    if respTime &gt;= (dur_inhaleIHOH+durHold):&amp;#10;    #if not exhale and respTime &gt;= (dur_inhaleIHOH+durHold) and HV_loop.thisN+1 &lt; HV_loops:&amp;#10;        exhale = True&amp;#10;        #print(&quot;EXHALE - current info HV_loop.thisN, HV_loops:&quot;, (HV_loop.thisN, HV_loops))&amp;#10;        #   #another (slower/faster) x-shifted (=move left/right) &quot;cos()&quot;-function&amp;#10;        circ_dia = res_size -cosXYshift*cos(pi*respTime/(dur_exhaleIHOH+durHold) + shift_x) + cosXYshift&amp;#10;&amp;#10;        circle_color = exhale_color&amp;#10;        resp_txt = 'Ausatmen'&amp;#10;&amp;#10;except Exception as e:&amp;#10;    # Log or handle exceptions here&amp;#10;    print(f&quot;An error occurred: {e}&quot;)&amp;#10;&amp;#10;&quot;&quot;&quot;&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if ((! first)) {&amp;#10;    startTime = t;&amp;#10;    first = true;&amp;#10;}&amp;#10;if ((! (INHALE.status === PsychoJS.Status.FINISHED))) {&amp;#10;    bar_size = ((t - startTime) * (bar_height / dur_inhaleIHOH));&amp;#10;}&amp;#10;if (((inHOLD.status === PsychoJS.Status.FINISHED) &amp;&amp; (! second))) {&amp;#10;    startTime = t;&amp;#10;    slider_color = exhale_color;&amp;#10;    second = true;&amp;#10;}&amp;#10;if (((inHOLD.status === PsychoJS.Status.FINISHED) &amp;&amp; (! (EXHALE.status === PsychoJS.Status.FINISHED)))) {&amp;#10;    bar_size = (bar_height - ((t - startTime) * (bar_height / dur_exhaleIHOH)));&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="win.mouseVisible = True&amp;#10;&amp;#10;# ===== END ROUTINE Tab (Optional) =====&amp;#10;# Dieser Code kommt in den &quot;End Routine&quot; Tab - nur falls du aufräumen möchtest&amp;#10;&amp;#10;# Optional: Variablen löschen um Speicher freizugeben&amp;#10;# del breathing_circle&amp;#10;# del breathing_text&amp;#10;# del breathing_clock&amp;#10;&amp;#10;print(f&quot;=== ATEMBALL ROUTINE BEENDET nach {breathing_clock.getTime():.1f}s ===&quot;)&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
