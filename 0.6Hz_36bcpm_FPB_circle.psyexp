      <CodeComponent name="HVcode" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="##  respiration timer  ##&amp;#10;respClock = core.Clock()&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="bar_posX = ((- bar_height) / 2);&amp;#10;bar_size = 0;&amp;#10;slider_color = inhale_color;&amp;#10;first = false;&amp;#10;second = false;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="win.mouseVisible = False&amp;#10;&amp;#10;c_resp_txt.alignText = 'center'&amp;#10;&amp;#10;##  &gt; 'Einatmen'&amp;#10;resp_txt = 'Einatmen'&amp;#10;b_txt_size = 0.035&amp;#10;&amp;#10;circle_color = inhale_color         #(re)set color of breathing bar&amp;#10;&amp;#10;respTime = 0&amp;#10;respClock.reset()&amp;#10;&amp;#10;exhale = False&amp;#10;&amp;#10;HVonce = False&amp;#10;#print(&quot;current HV-loop:&quot;, (HV_loop.thisN, HV_loops, HV_loop.thisTrialN))&amp;#10;&amp;#10;tonce = False&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Py" valType="str" updates="None" name="Code Type"/>
        <Param val="try:&amp;#10;    ##  get current (respiration-animation) time  ##&amp;#10;    respTime = respClock.getTime()&amp;#10;&amp;#10;    # # # respiration text: basic-size = 0.035  ##&amp;#10;    # # #    '/50' &gt; flatten the (cos-)curve; +1/50 &gt; offset to start x=0|y=0&amp;#10;    # # # b_txt_size = 0.035 -cos(pi*respTime/(dur_inhaleIHOH+durHold))/50 +1/50 #&amp;#10;&amp;#10;    ##  respiration circle: res(idual)-size = 200px  ##&amp;#10;    ##    '+|- '150' &gt; variability of circle size&amp;#10;    circ_dia = res_size -cosXYshift*cos(pi*respTime/(dur_inhaleIHOH+durHold)) + cosXYshift&amp;#10;&amp;#10;    ##  &gt; hold&amp;#10;    #if respTime &gt;= dur_inhaleIHOH: resp_txt = ' '  #no 'hold' for HV&amp;#10;&amp;#10;    ##  &gt; 'Ausatmen'&amp;#10;    if respTime &gt;= (dur_inhaleIHOH+durHold):&amp;#10;    #if not exhale and respTime &gt;= (dur_inhaleIHOH+durHold) and HV_loop.thisN+1 &lt; HV_loops:&amp;#10;        exhale = True&amp;#10;        #print(&quot;EXHALE - current info HV_loop.thisN, HV_loops:&quot;, (HV_loop.thisN, HV_loops))&amp;#10;        #   #another (slower/faster) x-shifted (=move left/right) &quot;cos()&quot;-function&amp;#10;        circ_dia = res_size -cosXYshift*cos(pi*respTime/(dur_exhaleIHOH+durHold) + shift_x) + cosXYshift&amp;#10;&amp;#10;        circle_color = exhale_color&amp;#10;        resp_txt = 'Ausatmen'&amp;#10;&amp;#10;except Exception as e:&amp;#10;    # Log or handle exceptions here&amp;#10;    print(f&quot;An error occurred: {e}&quot;)&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if ((! first)) {&amp;#10;    startTime = t;&amp;#10;    first = true;&amp;#10;}&amp;#10;if ((! (INHALE.status === PsychoJS.Status.FINISHED))) {&amp;#10;    bar_size = ((t - startTime) * (bar_height / dur_inhaleIHOH));&amp;#10;}&amp;#10;if (((inHOLD.status === PsychoJS.Status.FINISHED) &amp;&amp; (! second))) {&amp;#10;    startTime = t;&amp;#10;    slider_color = exhale_color;&amp;#10;    second = true;&amp;#10;}&amp;#10;if (((inHOLD.status === PsychoJS.Status.FINISHED) &amp;&amp; (! (EXHALE.status === PsychoJS.Status.FINISHED)))) {&amp;#10;    bar_size = (bar_height - ((t - startTime) * (bar_height / dur_exhaleIHOH)));&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
