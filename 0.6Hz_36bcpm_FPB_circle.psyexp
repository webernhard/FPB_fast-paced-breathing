      <CodeComponent name="HVnew_code" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="##  respiration timer  ##&amp;#10;#respClock = core.Clock()&amp;#10;&amp;#10;# Timer für diese Routine&amp;#10;breathing_clock = core.Clock()&amp;#10;&amp;#10;# Dauer der Atemball-Routine in Sekunden&amp;#10;breathing_duration = 10.0  # oder wie lange du willst&amp;#10;&amp;#10;# Erstelle die visuellen Komponenten (nutzt das bestehende win-Objekt)&amp;#10;breathing_circle = visual.Circle(&amp;#10;    win,&amp;#10;    #win=win,&amp;#10;    radius=MAX_RADIUS * MIN_SIZE_PERCENT,&amp;#10;    edges=64,&amp;#10;    lineColor=[1, 1, 1],&amp;#10;    lineWidth=3,&amp;#10;    pos=[0, 0]  # Position anpassen falls nötig&amp;#10;)&amp;#10;&amp;#10;breathing_text = visual.TextStim(&amp;#10;    win,&amp;#10;    #win=win,&amp;#10;    text='',&amp;#10;    pos=[0, -350],  # Position unter dem Ball&amp;#10;    #height=40, #px&amp;#10;    height=.35, # unit: norm&amp;#10;    color=[1, 1, 1]&amp;#10;)" valType="extendedCode" updates="constant" name="Begin Experiment"/>
